<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.baoyz.viewbinder.sample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.baoyz.viewbinder.sample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.baoyz.viewbinder.sample.SimpleActivity" >
        </activity>
        <activity android:name="com.baoyz.viewbinder.sample.ContactActivity" >
        </activity>
        <activity android:name="com.baoyz.viewbinder.sample.SimpleDetailActivity" >
        </activity>
        <activity android:name="com.baoyz.viewbinder.sample.SetterSampleActivity" >
        </activity>
    </application>

</manifest>